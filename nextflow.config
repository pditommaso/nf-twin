profiles {
  standard {
    process.container = 'quay.io/nextflow/bash'
    executor.cpus = 1 
  }
  
  timed {
      process.time = 1.min
  }

  required {
      process {
          withName: foo {  machineType = 'mem1_ssd1_x2'  }
          withName: bar {  machineType = 'mem2_ssd1_v2_x2'  }
      }
  }
}

